{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#8AADF4",
    "cream": "#F9F6F0",
    "deeppurp": "#191f48",
    "deepgreen": "#44992b ",
    "othpurp": "#4800bf",
    "lavender": "#B7BDF8",
    "orange": "#FA931E",
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "text": "#FFE551",
    "wight": "#FFFFFF"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{.Icon}} ",
          "foreground": "p:orange",
          "powerline_symbol": "\ue0b4",
          "background": "p:cream",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }}",
          "foreground": "p:orange",
          "powerline_symbol": "\ue0b4",
          "background": "p:cream",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }}",
          "foreground": "p:orange",
          "powerline_symbol": "\ue0b4",
          "background": "p:deeppurp",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": " {{ .HEAD }}",
          "foreground": "p:orange",
          "powerline_symbol": "\ue0b4",
          "background": "p:cream",
          "type": "git",
          "style": "powerline"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
